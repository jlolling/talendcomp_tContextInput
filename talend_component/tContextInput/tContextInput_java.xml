<?xml version="1.0" encoding="UTF-8"?>

<COMPONENT> 
  <HEADER AUTHOR="cimt AG" COMPATIBILITY="ALL" DATA_AUTO_PROPAGATE="false" PLATEFORM="ALL" RELEASE_DATE="20210125" SERIAL="" STARTABLE="true" STATUS="RELEASE" VERSION="3.1"> 
    <SIGNATURE/> 
  </HEADER>  
  <FAMILIES> 
    <FAMILY>Misc</FAMILY> 
  </FAMILIES>  
  <DOCUMENTATION> 
    <URL/> 
  </DOCUMENTATION>  
  <CONNECTORS> 
    <CONNECTOR CTYPE="ITERATE" MAX_INPUT="1"/>  
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" MAX_OUTPUT="1"/>  
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" MAX_OUTPUT="1"/>  
    <CONNECTOR CTYPE="COMPONENT_OK"/>  
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>  
    <CONNECTOR CTYPE="RUN_IF"/>  
    <CONNECTOR NAME="CONTEXT_VARIABLES" CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1" LINE_STYLE="1"/> 
  </CONNECTORS>  
  <PARAMETERS> 
    <PARAMETER NAME="SCHEMA_CONTEXT_VARIABLES" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="5" CONTEXT="CONTEXT_VARIABLES"> 
      <TABLE READONLY="true"> 
        <COLUMN NAME="VAR_NAME" TYPE="id_String" READONLY="true" CUSTOM="true" LENGTH="128" KEY="true" NULLABLE="false" COMMENT="Name of the context variable"/>  
        <COLUMN NAME="VAR_VALUE" TYPE="id_String" READONLY="true" CUSTOM="true" KEY="false" NULLABLE="true" COMMENT="Value of the context variable"/>  
        <COLUMN NAME="ORIGINAL_VAR_VALUE" TYPE="id_String" READONLY="true" CUSTOM="true" KEY="false" NULLABLE="true" COMMENT="Value of the context variable before something was replaced"/>  
        <COLUMN NAME="CONFIGURED_IN_CURR_JOB" TYPE="id_Boolean" READONLY="true" CUSTOM="true" LENGTH="255" NULLABLE="true" COMMENT="Var is configured in current job"/>  
        <COLUMN NAME="IS_PROMPT" TYPE="id_Boolean" READONLY="true" CUSTOM="true" LENGTH="255" NULLABLE="true" COMMENT="Var is a prompt var"/>  
        <COLUMN NAME="SOURCE_FILE" TYPE="id_String" READONLY="true" CUSTOM="true" LENGTH="255" NULLABLE="true" COMMENT="From which file the value was finally load"/> 
      </TABLE> 
    </PARAMETER>  
    <PARAMETER NAME="LOAD_CONTEXT_FILES" FIELD="CHECK" NUM_ROW="10"> 
      <DEFAULT>false</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="LOAD_HINT" FIELD="LABEL" COLOR="0;0;0" NUM_ROW="11" SHOW_IF="LOAD_CONTEXT_FILES == 'true'"> 
      <DEFAULT>Take care the files are encoded in ISO 8859-1</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="LOAD_SINGLETON" FIELD="CHECK" NUM_ROW="10"> 
      <DEFAULT>true</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="CONTEXT_FILE_FILTERS" FIELD="TABLE" NUM_ROW="20" SHOW_IF="LOAD_CONTEXT_FILES == 'true'"> 
      <ITEMS> 
        <ITEM NAME="FILE_FILTER" FIELD="TEXT" VALUE="&quot;/path/to/dir/or/file&quot;"/>  
        <ITEM NAME="IGNORE_MISSING" FIELD="CHECK" VALUE="false"/> 
      </ITEMS> 
    </PARAMETER>  
    <PARAMETER NAME="APPLY_REPLACEMENTS_TO_FILTER" FIELD="CHECK" NUM_ROW="25" SHOW_IF="LOAD_CONTEXT_FILES == 'true'"> 
      <DEFAULT>false</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="DECRYPT_PASSWORDS" FIELD="CHECK" NUM_ROW="30" SHOW_IF="LOAD_CONTEXT_FILES == 'true'"> 
      <DEFAULT>false</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="SET_MASTER_PASSWORD" FIELD="CHECK" NUM_ROW="31" SHOW_IF="LOAD_CONTEXT_FILES == 'true'"> 
      <DEFAULT>false</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="PW_HINT" FIELD="LABEL" COLOR="0;0;0" NUM_ROW="32" SHOW_IF="(LOAD_CONTEXT_FILES == 'true') and (SET_MASTER_PASSWORD == 'true')"> 
      <DEFAULT>This affects also all other tContextInput components in this job hierarchy. It enough to set it in the very first tContextInput component.</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="MASTER_PASSWORD" FIELD="PASSWORD" NUM_ROW="35" REQUIRED="true" SHOW_IF="(LOAD_CONTEXT_FILES == 'true') and (SET_MASTER_PASSWORD == 'true')"> 
      <DEFAULT/> 
    </PARAMETER>  
    <PARAMETER NAME="ALLOW_FILE_INCLUDES" FIELD="CHECK" NUM_ROW="40" SHOW_IF="LOAD_CONTEXT_FILES == 'true'"> 
      <DEFAULT>false</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="FILE_INCLUDE_KEY_PATTERN" FIELD="TEXT" REQUIRED="true" NUM_ROW="40" SHOW_IF="(LOAD_CONTEXT_FILES == 'true') and (ALLOW_FILE_INCLUDES == 'true')"> 
      <DEFAULT>"^file_*"</DEFAULT> 
    </PARAMETER> 
  </PARAMETERS>  
  <ADVANCED_PARAMETERS> 
    <PARAMETER NAME="PARAM_REPLACEMENT" FIELD="TABLE" NUM_ROW="20" NB_LINES="10"> 
      <ITEMS> 
        <ITEM NAME="PLACEHOLDER" FIELD="String" VALUE="&quot;{parameterName}&quot;"/>  
        <ITEM NAME="VALUE" FIELD="String" VALUE="&quot;parameterValue&quot;"/>  
        <ITEM NAME="VALUE_CASE_CHANGE" FIELD="CLOSED_LIST"> 
          <ITEMS DEFAULT="UNCHANGED"> 
            <ITEM NAME="UNCHANGED" VALUE="UNCHANGED"/>  
            <ITEM NAME="UPPERCASE" VALUE="UPPERCASE"/>  
            <ITEM NAME="LOWERCASE" VALUE="LOWERCASE"/> 
          </ITEMS> 
        </ITEM> 
      </ITEMS> 
    </PARAMETER>  
    <PARAMETER NAME="RELEASE_LABEL_20210125" FIELD="LABEL" COLOR="0;0;0" NUM_ROW="900">
      <DEFAULT>Release: 3.1 build at: 20210125</DEFAULT>
    </PARAMETER>
  </ADVANCED_PARAMETERS>  
  <CODEGENERATION> 
    <IMPORTS> 
      <IMPORT NAME="jlo-talendcomp-contextinput" MODULE="jlo-talendcomp-contextinput-3.1.jar" MVN="mvn:org.talend.libraries/jlo-talendcomp-contextinput-3.1/6.0.0-SNAPSHOT" REQUIRED="true"/>
      <IMPORT NAME="commons-io" MODULE="commons-io-1.3.2.jar" MVN="mvn:org.talend.libraries/commons-io-1.3.2/6.0.0-SNAPSHOT" REQUIRED="true"/>
    </IMPORTS> 
  </CODEGENERATION>  
  <RETURNS> 
    <RETURN AVAILABILITY="AFTER" NAME="NB_LINE" TYPE="id_Integer"/> 
  </RETURNS>
</COMPONENT>
